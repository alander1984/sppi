<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog /xsd/dbchangelog-3.6.xsd">
    <changeSet id="001" author="ykorzhev">
        <update tableName="sppi_questions">
            <column name="note" value="Указывается результат количественного benchmark-тестирования."/>
            <column name="content" value="Результат benchmark-теста?"/>
            <column name="answers" value="{'yes':'Пройден','no':'Не пройден'}"/>
            <column name="code" value="UPD15"/>
            <where>code = 'UPD26'</where>
        </update>
    </changeSet>
</databaseChangeLog>
