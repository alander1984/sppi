import com.egartech.sppi.model.Step;

rule "Yes to first question"
    when
        step:Step(current.code=="FIRST"&&answer=="yes");
    then
        step.setNextQ("SECOND");
        update(step);
end

rule "No to first question"
    when
        step:Step(current.code=="FIRST"&&answer=="no");
    then
        step.setNextQ("THIRD");
        update(step);
end